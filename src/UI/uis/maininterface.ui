   <string notr="true">/* 专业深色配色方案 - 三种颜色 */
/* 深灰 #1E1E1E | 中灰 #2D2D2D | 深蓝绿 #2D4F67 */

QWidget {
    background-color: #1E1E1E;
    color: #D4D4D4;
    font-family: &quot;Segoe UI&quot;, &quot;Microsoft YaHei&quot;, sans-serif;
    font-size: 13px;
    border: none;
}

/* 标题样式 */
QLabel#titleLabel {
    font-size: 16px;
    font-weight: bold;
    color: #FFFFFF;
    background-color: #2D2D2D;
    padding: 12px 0;
    border-bottom: 1px solid #3C3C3C;
}

/* 普通标签样式 */
QLabel {
    font-size: 13px;
    color: #D4D4D4;
    padding: 2px 0;
    background-color: transparent;
}

/* 数值显示标签 */
QLabel#fpsShow,
QLabel#CPUpayloadShow,
QLabel#memoryUsageShow,
QLabel#confidenceValueLabel,
QLabel#exposureValueLabel,
QLabel#mirrorModeLabel {
    font-size: 13px;
    font-weight: bold;
    color: #4EC9B0;
    background-color: #2D2D2D;
    padding: 4px 8px;
    border-radius: 3px;
    border: 1px solid #3C3C3C;
    min-width: 50px;
    max-width: 50px;
    text-align: center;
}

QLabel#mirrorModeLabel {
    min-width: 100px;
    max-width: 100px;
}

/* 主要按钮样式 */
QPushButton {
    background-color: #2D2D2D;
    color: #D4D4D4;
    border: 1px solid #3C3C3C;
    padding: 8px 12px;
    border-radius: 3px;
    font-weight: normal;
    font-size: 13px;
    min-height: 32px;
    min-width: 80px;
    max-width: 120px;
}

QPushButton:hover {
    background-color: #3C3C3C;
    border-color: #2D4F67;
}

QPushButton:pressed {
    background-color: #2D4F67;
    color: #FFFFFF;
}

/* 录像/拍照按钮特殊样式 */
QPushButton#actionButton {
    background-color: #8B3519;
    color: #FFFFFF;
    border: 1px solid #A53E1F;
    font-weight: bold;
    min-width: 100px;
    max-width: 100px;
}

QPushButton#actionButton:hover {
    background-color: #A53E1F;
    border-color: #BF4826;
}

QPushButton#actionButton:pressed {
    background-color: #6D2A14;
}

/* 模式切换按钮 */
QPushButton#modeToggleButton {
    background-color: #2D4F67;
    color: #FFFFFF;
    border: 1px solid #3A6585;
    font-weight: bold;
    min-width: 80px;
    max-width: 80px;
    min-height: 36px;
}

QPushButton#modeToggleButton:hover {
    background-color: #3A6585;
    border-color: #4A7A9F;
}

QPushButton#modeToggleButton:pressed {
    background-color: #234158;
}

/* 小控制按钮 */
QPushButton#confidenceSUB,
QPushButton#confidenceADD,
QPushButton#exposureSUB,
QPushButton#exposureADD,
QPushButton#mirrorLeftButton,
QPushButton#mirrorRightButton {
    background-color: #2D2D2D;
    color: #D4D4D4;
    border: 1px solid #3C3C3C;
    min-width: 28px;
    min-height: 28px;
    max-width: 28px;
    max-height: 28px;
    padding: 0px;
    border-radius: 3px;
    font-weight: bold;
    font-size: 12px;
}

QPushButton#confidenceSUB:hover,
QPushButton#confidenceADD:hover,
QPushButton#exposureSUB:hover,
QPushButton#exposureADD:hover,
QPushButton#mirrorLeftButton:hover,
QPushButton#mirrorRightButton:hover {
    background-color: #3C3C3C;
    border-color: #2D4F67;
}

QPushButton#confidenceSUB:pressed,
QPushButton#confidenceADD:pressed,
QPushButton#exposureSUB:pressed,
QPushButton#exposureADD:pressed,
QPushButton#mirrorLeftButton:pressed,
QPushButton#mirrorRightButton:pressed {
    background-color: #2D4F67;
    color: #FFFFFF;
}

/* 滑块样式 */
QSlider::groove:horizontal {
    border: 1px solid #3C3C3C;
    height: 4px;
    background-color: #2D2D2D;
    border-radius: 2px;
}

QSlider::handle:horizontal {
    background-color: #2D4F67;
    border: 2px solid #1E1E1E;
    width: 14px;
    height: 14px;
    border-radius: 7px;
    margin: -5px 0;
}

QSlider::handle:horizontal:hover {
    background-color: #3A6585;
}

QSlider::sub-page:horizontal {
    background-color: #2D4F67;
    border-radius: 2px;
}

/* 分割线样式 */
Line {
    background-color: #3C3C3C;
    color: #3C3C3C;
    border: none;
    min-height: 1px;
    max-height: 1px;
}

/* 版本信息标签 */
QLabel#versionLabel {
    color: #808080;
    font-size: 11px;
    padding: 8px 0;
    background-color: transparent;
}

/* 布局容器 */
QWidget#widget {
    background-color: #1E1E1E;
    border: 1px solid #3C3C3C;
}
